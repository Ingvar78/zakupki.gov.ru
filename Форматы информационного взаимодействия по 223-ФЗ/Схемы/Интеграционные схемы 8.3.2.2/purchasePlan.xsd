<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2008 (http://www.altova.com) by A (A) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://zakupki.gov.ru/223fz/purchasePlan/1" xmlns:t="http://zakupki.gov.ru/223fz/types/1" targetNamespace="http://zakupki.gov.ru/223fz/purchasePlan/1" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="http://zakupki.gov.ru/223fz/types/1" schemaLocation="Types.xsd"/>
	<xs:element name="purchasePlan">
		<xs:annotation>
			<xs:documentation>План закупки</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="t:packetType">
					<xs:sequence>
						<xs:element name="body">
							<xs:annotation>
								<xs:documentation>Данные информационного пакета</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="item" type="purchasePlanItemType">
										<xs:annotation>
											<xs:documentation>Позиция в информационном пакете</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="basePurchasePlanDataType" abstract="true">
		<xs:annotation>
			<xs:documentation>План закупки</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="guid" type="t:guidType">
				<xs:annotation>
					<xs:documentation>Глобальный идентификатор плана закупки</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customer" type="t:customerInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Данные заказчика</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="placer" type="t:customerInfoType">
				<xs:annotation>
					<xs:documentation>Данные организации, создавшей сведения плана закупки</xs:documentation>
				</xs:annotation>
			</xs:element>
            <xs:element name="planType" type="planType">
                <xs:annotation>
                    <xs:documentation>Вид плана закупки</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:choice>
                <xs:element name="purchasePlanItems" type="purchasePlanItemsListType" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Позиции плана</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="innovationPlanItems" type="innovationPlanItemsListType" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Позиции плана инновационной закупки</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
            <xs:choice>
				<xs:element name="purchasePlanItemsSMB" type="purchasePlanItemsListType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Позиции плана субъектов малого и среднего бизнеса</xs:documentation>
					</xs:annotation>
				</xs:element>
                <xs:element name="innovationPlanItemsSMB" type="innovationPlanItemsListType" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Позиции проекта плана инновационной закупки субъектов малого и среднего бизнеса</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
            <xs:element name="isUploadComplete" type="xs:boolean" default="false" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Признак «Загрузка завершена»</xs:documentation>
                </xs:annotation>
            </xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="purchasePlanDataType">
		<xs:annotation>
			<xs:documentation>План закупки</xs:documentation>
		</xs:annotation>
        <xs:complexContent>
            <xs:extension base="basePurchasePlanDataType">
		<xs:sequence>
			<xs:element name="createDateTime" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Дата создания плана</xs:documentation>
				</xs:annotation>
			</xs:element>
            <xs:element name="urlEIS" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Url документа в ЕИС</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="t:nonEmptyString">
                        <xs:maxLength value="3000"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
			<xs:element name="urlVSRZ" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Url документа в ВСРЗ</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="t:nonEmptyString">
						<xs:maxLength value="3000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="registrationNumber" type="t:registrationNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Регистрационный номер плана</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Наименование плана закупки</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="t:nonEmptyString">
						<xs:maxLength value="2000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="additionalInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дополнительные сведения</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="t:nonEmptyString">
						<xs:maxLength value="2000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="startDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>Дата начала действия плана</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="endDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>Дата окончания действия плана</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="approveDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>Дата утверждения плана</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="publicationDateTime" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата размещения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="isDigitForm" type="xs:boolean" default="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Способ внесения информации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="summSizeCh15" type="xs:boolean" default="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Признак, что закупки осуществляются на сумму, не превышающую размер, установленный в части 15 статьи 4 Закона № 223-ФЗ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="isImportedFromVSRZ" type="xs:boolean" default="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Признак импорта из ВСРЗ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="t:purchasePlanStatusType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Статус плана</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="version" type="t:versionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер редакции</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="modificationDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Обоснование внесения изменений</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="t:nonEmptyString">
						<xs:maxLength value="2000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
            <xs:element name="useNewClassifiers" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Используются новые классификаторы ОКПД2 и ОКВЭД2</xs:documentation>
                </xs:annotation>
            </xs:element>
			<xs:element name="attachments" type="t:documentListType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Прикрепленные к сведениям документы</xs:documentation>
				</xs:annotation>
			</xs:element>
            <xs:element name="excludeVolume" type="t:money2" minOccurs="0">
            	<xs:annotation>
            		<xs:documentation>
            			Совокупный годовой объем планируемых закупок товаров, работ, услуг, 
            			которые исключаются при расчете годового объема закупок товаров, работ, услуг, 
            			которые планируется осуществить по результатам закупки товаров, работ, услуг, 
            			участниками которой являются только субъекты малого и среднего предпринимательства
            		</xs:documentation>
            	</xs:annotation>
            </xs:element>
            <xs:element name="volumeSMB" type="t:money2" minOccurs="0">
            	<xs:annotation>
            		<xs:documentation>
            			Годовой объем закупок, которые планируется осуществить по результатам закупки, 
            			участниками которой являются только субъекты малого и среднего предпринимательства.
					</xs:documentation>
            	</xs:annotation>
            </xs:element>
            <xs:element name="annualVolume" type="t:money2" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Совокупный годовой объем планируемых закупок товаров (работ,услуг)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="percentSMB" type="xs:decimal" minOccurs="0">
            	<xs:annotation>
            		<xs:documentation>Процент</xs:documentation>
            	</xs:annotation>
            </xs:element>
            <xs:element name="smbPartitionChanged" type="xs:boolean" default="false" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Изменён раздел МСП</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="annualVolumeSMBLess15Percent" type="xs:boolean" default="false" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Годовой объём закупок МСП менее 15%</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="reportingYear" type="xs:int">
                <xs:annotation>
                    <xs:documentation>Отчетный год</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="previousYearAnnualVolume" type="t:money2" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        СГОЗ за предыдущий год
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="previousYearAnnualVolumeHiTech" type="t:money2" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        СГОЗ ИПВП за предыдущий год
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="previousYearAnnualVolumeHiTechSMB" type="t:money2" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        СГОЗ ИПВП МСП за предыдущий год
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="annualVolumeHiTechSumm" type="t:money2" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        СГОЗ ИПВП (сумма)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="annualVolumeHiTechIncrease" type="t:money2" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        СГОЗ ИПВП (увеличение)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="annualVolumeHiTechPercent" type="xs:decimal" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        СГОЗ ИПВП (доля)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="annualVolumeHiTechSMBSumm" type="t:money2" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        СГОЗ ИПВП МСП (сумма)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="annualVolumeHiTechSMBIncrease" type="t:money2" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        СГОЗ ИПВП МСП (увеличение)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="annualVolumeHiTechSMBPercent" type="xs:decimal" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        СГОЗ ИПВП МСП (доля)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
		</xs:sequence>
            </xs:extension>
		</xs:complexContent>
	</xs:complexType>

    <xs:complexType name="longTermVolumeType">
        <xs:annotation>
            <xs:documentation>Объём оплаты долгосрочного договора</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="details" type="longTermVolumeDetailListType" minOccurs="0" maxOccurs="1">
            	<xs:annotation>
            		<xs:documentation>Детализация объёмов оплаты долгосрочного договора по годам</xs:documentation>
            	</xs:annotation>
            </xs:element>
            <xs:element name="volume" type="t:money2" minOccurs="0" maxOccurs="1">
            	<xs:annotation>
            		<xs:documentation>Объём оплаты долгосрочного договора за все года исполнения долгосрочного договора</xs:documentation>
            	</xs:annotation>
            </xs:element>
            <xs:element name="volumeRub" type="t:money2" minOccurs="0" maxOccurs="1">
            	<xs:annotation>
            		<xs:documentation>Объём оплаты долгосрочного договора в рублевом эквиваленте за все года исполнения долгосрочного договора</xs:documentation>
            	</xs:annotation>
            </xs:element>
            <xs:element name="currency" type="t:currencyType" minOccurs="0" maxOccurs="1">
            	<xs:annotation>
            		<xs:documentation>Валюта объёма привлечения субъектов малого и среднего предпринимательства</xs:documentation>
            	</xs:annotation>
            </xs:element>
            <xs:element name="exchangeRate" type="t:money6" minOccurs="0" maxOccurs="1">
            	<xs:annotation>
            		<xs:documentation>Курс валюты объёма привлечения субъектов малого и среднего предпринимательства</xs:documentation>
            	</xs:annotation>
            </xs:element>
            <xs:element name="exchangeRateDate" type="xs:date" minOccurs="0" maxOccurs="1">
            	<xs:annotation>
            		<xs:documentation>Дата, на которую установлен курс валюты объёма привлечения субъектов малого и среднего предпринимательства</xs:documentation>
            	</xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="longTermVolumeDetailListType">
        <xs:annotation>
            <xs:documentation>Объёмы оплаты долгосрочного договора по годам</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="longTermVolumeDetail" type="longTermVolumeDetailType" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Объём оплаты долгосрочного договора за год</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="longTermVolumeDetailType">
        <xs:annotation>
            <xs:documentation>Объём оплаты долгосрочного договора за год</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="year" type="xs:int">
            	<xs:annotation>
            		<xs:documentation>Год планируемого платежа</xs:documentation>
            	</xs:annotation>
            </xs:element>
            <xs:element name="summ" type="t:money2" minOccurs="0">
            	<xs:annotation>
            		<xs:documentation>Сумма планируемого платежа</xs:documentation>
            	</xs:annotation>
            </xs:element>
            <xs:element name="summRub" type="t:money2" minOccurs="0">
            	<xs:annotation>
            		<xs:documentation>Сумма планируемого платежа в рублевом эквиваленте</xs:documentation>
            	</xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="basePlanDataItemType" abstract="true">
        <xs:annotation>
            <xs:documentation>Позиция плана</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="guid" type="t:guidType">
                <xs:annotation>
                    <xs:documentation>Глобальный идентификатор позиции плана</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ordinalNumber" type="xs:int">
                <xs:annotation>
                    <xs:documentation>Номер позиции</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="contractSubject">
                <xs:annotation>
                    <xs:documentation>Наименование предмета договора</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="t:nonEmptyString">
                        <xs:maxLength value="2000"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="planItemCustomer" type="t:customerInfoType">
                <xs:annotation>
                    <xs:documentation>Данные заказчика для позиции закупки</xs:documentation>
                </xs:annotation>
    		</xs:element>
            <xs:element name="minimumRequirements" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Минимальные требования</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:whiteSpace value="collapse"/>
                        <xs:maxLength value="2000"/>
					</xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="contractEndDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Срок исполнения договора</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="additionalInfo" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дополнительные сведения</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="t:nonEmptyString">
                        <xs:maxLength value="1000"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="modificationDescription" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Обоснование внесения изменений</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="t:nonEmptyString">
                        <xs:maxLength value="2000"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="status" type="t:purchasePlanItemStatusType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Статус позиции</xs:documentation>
                </xs:annotation>
    		</xs:element>
            <xs:element name="isPurchasePlaced" type="xs:boolean" minOccurs="0">
            	<xs:annotation>
            		<xs:documentation>Признак Закупка размещена</xs:documentation>
            	</xs:annotation>
            </xs:element>
            <xs:element name="changedGWSAndDates" type="xs:boolean" minOccurs="0">
            	<xs:annotation>
            		<xs:documentation>Изменение ТРУ и сроков</xs:documentation>
            	</xs:annotation>
            </xs:element>
            <xs:element name="changedNMSKMoreTenPercent" type="xs:boolean" minOccurs="0">
            	<xs:annotation>
            		<xs:documentation>Изменение более чем на 10 процентов НМЦД</xs:documentation>
            	</xs:annotation>
            </xs:element>
            <xs:element name="otherChanges" type="xs:boolean" minOccurs="0">
            	<xs:annotation>
            		<xs:documentation>Изменение в иных случаях</xs:documentation>
            	</xs:annotation>
            </xs:element>
            <xs:element name="checkResult" type="purchasePlanItemCheckResultType" minOccurs="0">
            	<xs:annotation>
            		<xs:documentation>Результат проверки</xs:documentation>
            	</xs:annotation>
            </xs:element>
			<xs:element name="errorMessages" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сообщения об ошибках</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="t:nonEmptyString">
						<xs:maxLength value="4000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
            <xs:element name="cancellationReason" type="cancellationReasonType" minOccurs="0">
            	<xs:annotation>
            		<xs:documentation>Причина аннулирования</xs:documentation>
            	</xs:annotation>
            </xs:element>
            <xs:element name="longTerm" type="xs:boolean" default="false" minOccurs="0">
            	<xs:annotation>
            		<xs:documentation>Признак "Позиция является долгосрочной"</xs:documentation>
            	</xs:annotation>
            </xs:element>
            <xs:element name="shared" type="xs:boolean" minOccurs="0" maxOccurs="1">
            	<xs:annotation>
            		<xs:documentation>Признак "Позиция учитывается в нескольких планах закупки"</xs:documentation>
            	</xs:annotation>
            </xs:element>
            <xs:element name="initialPositionData" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Глобальный идентификатор позиции исходного плана закупки и глобальный идентификатор версии исходного плана (для позиций с признаком "Позиция учитывается в нескольких планах закупки")</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="initialPositionGuid" type="t:guidType" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Глобальный идентификатор позиции исходного плана закупки</xs:documentation>
                            </xs:annotation>
                        </xs:element>
