<?xml version="1.0" encoding="UTF-8"?>
<!-- ERUZ INTEGRATION TYPES TYPES Integration Scheme, version 9.2, create date 30.05.2019 -->
<xs:schema xmlns="http://zakupki.gov.ru/eruz/types/1" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:base="http://zakupki.gov.ru/eruz/base/1" xmlns:cmn="http://zakupki.gov.ru/eruz/common/1" targetNamespace="http://zakupki.gov.ru/eruz/types/1" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="http://zakupki.gov.ru/eruz/base/1" schemaLocation="eruzBaseTypes.xsd"/>
	<xs:import namespace="http://zakupki.gov.ru/eruz/common/1" schemaLocation="eruzCommonTypes.xsd"/>
	<!--Технические типы-->
	<xs:complexType name="confirmationType">
		<xs:annotation>
			<xs:documentation>Тип: Результат обработки</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="loadId" type="base:guidType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор загруженных данных</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="result" type="base:confirmationResultType">
				<xs:annotation>
					<xs:documentation>Результат обработки данных

success - обработано успешно;
failure - ошибки при обработке;
processing - обрабатывается.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="violations" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Выявленные в процессе обработки нарушения и ошибки</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="violation" type="violationType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="refId" type="base:guidType">
				<xs:annotation>
					<xs:documentation>Идентификатор обработанного пакета данных</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="missingIndexNums" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Список пропущенных порядковых номеров пакета данных в рамках передачи информации об объекте информационного обмена</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="indexNum" type="base:indexNumType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Порядковый номер пакета данных в рамках передачи информации об объекте информационного обмена</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemeVersion" type="base:schemeVersionType" use="required">
			<xs:annotation>
				<xs:documentation>Версия схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!--Основные документы-->
	<xs:complexType name="contractorType">
		<xs:annotation>
			<xs:documentation>Тип: Информация и документы об участнике закупки</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор документа ЕРУЗ.
Игнорируется при приёме, заполняется при передаче</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="versionNumber" type="base:versionNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер версии реестровой записи в ЕРУЗ. При приеме, если блок не заполнен, то считается, что принята первая версия</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commonInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Общая информация</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="registryNum" type="base:eruzRegistryNumType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Номер реестровой записи в ЕРУЗ.
Элемент игнорируется при приёме первой версии документа. Присваивается автоматически после размещения. Требуется обязательное заполнение при приёме изменения документа. Заполняется при передаче</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="registryDT" type="xs:dateTime" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Дата и время размещения (изменения) реестровой записи в ЕРУЗ.
Игнорируется при приёме, заполняется при передаче</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="startRegDate" type="xs:date" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Дата начала срока регистрации в ЕРУЗ.
Игнорируется при приёме, заполняется при передаче</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="endRegDate" type="xs:date" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Дата окончания срока регистрации в ЕРУЗ.
Игнорируется при приёме, заполняется при передаче</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="href" type="base:hrefType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Гиперссылка на карточку документа в ОЧ.
Игнорируется при приёме, заполняется при передаче</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="contractorInfo">
				<xs:annotation>
					<xs:documentation>Регистрационные данные.
При приеме контролируется обязательность заполнения блока "Юридическое лицо иностранного государства" (legalEntityForeignStateNotInRAFPInfo) или "Физическое лицо иностранного государства" (individualPersonForeignStateInfo)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice>
						<xs:element name="legalEntityRFInfo" type="legalEntityRFType">
							<xs:annotation>
								<xs:documentation>Юридическое лицо РФ</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="filialLegalEntityRFInfo" type="filialLegalEntityRFType">
							<xs:annotation>
								<xs:documentation>Филиал юридического лица РФ</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="individualPersonRFInfo" type="individualPersonRFType">
							<xs:annotation>
								<xs:documentation>Физическое лицо РФ</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="legalEntityForeignStateInRAFPInfo" type="legalEntityForeignStateInRAFPType">
							<xs:annotation>
								<xs:documentation>Аккредитованный филиал или представительство иностранного юридического лица</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="legalEntityForeignStateNotInRAFPInfo" type="legalEntityForeignStateNotInRAFPType">
							<xs:annotation>
								<xs:documentation>Юридическое лицо иностранного государства</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="individualPersonForeignStateInfo" type="individualPersonForeignStateType">
							<xs:annotation>
								<xs:documentation>Физическое лицо иностранного государства</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:complexType>
			</xs:element>
			<xs:element name="informationType" type="informationType">
				<xs:annotation>
					<xs:documentation>Тип передаваемой информации:
R - информация для/об аккредитации на ЭП;
P - информация для аккредитации на новый срок;
M - измененная информация
В случае, если в поле указано значение "R" или "P", в ответ должен быть направлен пакет "Информация об аккредитации участника закупки на ЭП" (eruzAccreditationResult)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ETP" type="cmn:ETPRef" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Электронная площадка. При приеме блок обязателен для заполнения, код контролируется на  присутствие в справочнике "Справочник: Электронные площадки " (nsiETP)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="printFormInfo" type="cmn:printFormType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Печатная форма документа. Игнорируется при приеме, заполняется при передаче</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="extPrintFormInfo" type="cmn:extPrintFormType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Электронный документ, полученный из внешней системы
При приеме контролируется обязательность заполнения блока, в котором будет содержаться xml, сформированный по схеме eruzPrintForm с типом contractor, либо ссылкой на этот xml из файлового хранилища ЕРУЗ. 
Не заполняется при передаче</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemeVersion" type="base:schemeVersionType" use="required">
			<xs:annotation>
				<xs:documentation>Версия схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="accreditationResultType">
		<xs:annotation>
			<xs:documentation>Тип: Информация об аккредитации участника закупки на ЭП</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="registryNum" type="base:eruzRegistryNumType">
				<xs:annotation>
					<xs:documentation>Номер реестровой записи в ЕРУЗ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ETP" type="cmn:ETPRef">
				<xs:annotation>
					<xs:documentation>Электронная площадка. При приеме код контролируется на  присутствие в справочнике "Справочник: Электронные площадки " (nsiETP)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="accreditationInfo">
				<xs:annotation>
					<xs:documentation>Информация об аккредитации</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice>
						<xs:element name="violations">
							<xs:annotation>
								<xs:documentation>В процессе обработки выявлены ошибки.
В случае если заполнен блок, то пакет игнорируется при приеме</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="violation" type="cmn:violationType" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>Ошибка</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="successRegister" type="xs:boolean" fixed="true">
							<xs:annotation>
								<xs:documentation>Поставщик аккредитован на ЭП</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemeVersion" type="base:schemeVersionType" use="required">
			<xs:annotation>
				<xs:documentation>Версия схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="contractorExcludeType">
		<xs:annotation>
			<xs:documentation>Тип: Информация об исключении участника закупки из ЕРУЗ</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Идентификатор документа ЕРУЗ.
Игнорируется при приёме, заполняется при передаче</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commonInfo">
				<xs:annotation>
					<xs:documentation>Общая информация</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="registryNum" type="base:eruzRegistryNumType">
							<xs:annotation>
								<xs:documentation>Номер реестровой записи в ЕРУЗ</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="publishedDT" type="xs:dateTime" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Дата и время размещения документа с информацией об исключении участника закупок.
Игнорируется при приёме, заполняется при передаче</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ETP" type="cmn:ETPRef" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Электронная площадка. При приеме блок обязателен для заполнения, код контролируется на  присутствие в справочнике "Справочник: Электронные площадки " (nsiETP)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="excludeReasonText" type="base:text500Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Причина исключения.
Не заполняется, если исключение участника закупок осуществлено в автоматическом режиме</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="excludeReason" type="cmn:contractorExcludeReasonRef">
				<xs:annotation>
					<xs:documentation>Основание исключения.
При приеме код контролируется на присутствие в справочнике "Справочник: Основания исключений" (nsiContractorExcludeReason)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="attachmentsInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Прикрепленная документация.
Игнорируется при приёме, заполняется при передаче при исключении участника закупки оператором сайта ЕИС</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="foundingDocsCopyInfo" type="cmn:attachmentListType">
							<xs:annotation>
								<xs:documentation>Копия судебного акта или акта судебного пристава-исполнителя, содержащие запрет на регистрацию участника закупки в единой информационной системе, или на размещение информации и документов в Едином реестре участников закупок.
Заполняется при исключении участника закупок оператором сайта ЕИС</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="printFormInfo" type="cmn:printFormType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Печатная форма документа. Игнорируется при приеме, заполняется при передаче</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="printFormFieldsInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дополнительная информация для печатной формы. Игнорируется при приёме, заполняется при передаче</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="fullName" type="base:text1000Type" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Полное наименование организации</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="INNOrganization" type="base:innOrganizationType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>ИНН организации</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="INNIndividual" type="base:innIndividualType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>ИНН физического лица</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="nameInfo" type="cmn:personType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>ФИО</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="taxPayerCode" type="base:taxPayerCode" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Аналог идентификационного номера налогоплательщика в соответствии с законодательством иностранного государства</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="extPrintFormInfo" type="cmn:extPrintFormType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Электронный документ, полученный из внешней системы. 
При приеме контролируется обязательность заполнения блока, в котором будет содержаться xml, сформированный по схеме eruzPrintForm с типом contractor, либо ссылкой на этот xml из файлового хранилища ЕРУЗ. 
Не заполняется при передаче</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemeVersion" type="base:schemeVersionType" use="required">
			<xs:annotation>
				<xs:documentation>Версия схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!--Вспомогательные типы-->
	<xs:complexType name="legalEntityRFType">
		<xs:annotation>
			<xs:documentation>Тип: Юридическое лицо РФ</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="legalEntityRFDataInfo">
				<xs:annotation>
					<xs:documentation>Регистрационные данные юридического лица РФ</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="legalEntityRFDataType">
							<xs:sequence>
								<xs:element name="OKOPF" type="cmn:OKOPFRef" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Сведения об организационно-правовой форме из ЕГРЮЛ</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="majorTaxPayerKPP" type="base:kppType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>КПП крупнейшего налогоплательщика</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="timeZoneInfo" type="cmn:timeZoneRef">
									<xs:annotation>
										<xs:documentation>Часовой пояс.
При приеме наименование контролируется на присутствие в справочнике "Часовые зоны мира" (nsiWorldTimeZone) </xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="maxPrice" type="base:moneyMaxLengthToPoint21Type">
									<xs:annotation>
										<xs:documentation>Максимально возможная сумма для заключения контракта по результатам электронных процедур</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="EGRULStatementInfo" type="cmn:attachmentType">
									<xs:annotation>
										<xs:documentation>Выписка из ЕГРЮЛ</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="isParticipant" type="xs:boolean">
									<xs:annotation>
										<xs:documentation>Участник закупок</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="isSubcontractor" type="xs:boolean">
									<xs:annotation>
										<xs:documentation>Субподрядная организация</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="managingOrganization" type="managingOrganizationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Управляющая организация</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="informationPersons" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения о лицах, имеющих право без доверенности действовать от имени юридического лица РФ</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="informationPersonInfo" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Сведения о лице, имеющем право без доверенности действовать от имени юридического лица</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="informationPersonType">
										<xs:sequence>
											<xs:element name="INN" type="base:innIndividualType" minOccurs="0">
												<xs:annotation>
													<xs:documentation>ИНН</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="contactInfo">
				<xs:annotation>
					<xs:documentation>Контактная информация</xs:documentation>
