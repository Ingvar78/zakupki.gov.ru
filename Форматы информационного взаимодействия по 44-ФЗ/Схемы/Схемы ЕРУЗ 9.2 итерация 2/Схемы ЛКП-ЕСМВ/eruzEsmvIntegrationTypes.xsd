<?xml version="1.0" encoding="UTF-8"?>
<!-- ERUZ ESMV INTEGRATION TYPES Integration Scheme, version 1.0, create date 19.03.2019 -->
<xs:schema xmlns="http://zakupki.gov.ru/eruz/esmv/types/1" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:base="http://zakupki.gov.ru/eruz/esmv/types/base/1" targetNamespace="http://zakupki.gov.ru/eruz/esmv/types/1" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="http://zakupki.gov.ru/eruz/esmv/types/base/1" schemaLocation="eruzEsmvBaseTypes.xsd"/>
	<!--Основные документы-->
	<!--Технические типы-->
	<xs:complexType name="tConfirmSinc">
		<xs:annotation>
			<xs:documentation>Тип: Предварительное подтверждение</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tConfirm"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tConfirmASinc">
		<xs:annotation>
			<xs:documentation>Тип: Окончательное подтверждение</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tConfirm"/>
		</xs:complexContent>
	</xs:complexType>
	<!--Лицевые счета-->
	<xs:complexType name="tRequestPA">
		<xs:annotation>
			<xs:documentation>Тип: Заявление на резервирование/открытие лицевого счёта</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="base:tGUID">
				<xs:annotation>
					<xs:documentation>Идентификатор документа ЕИС</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="commonInfo">
				<xs:annotation>
					<xs:documentation>Общая информация</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="requestNum" type="base:tApplicationNum" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Номер заявления</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="requestType" type="base:tRequestPAEnum">
							<xs:annotation>
								<xs:documentation>Тип заявления:
0 - Резервироование;
1 - Открытие</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="generationDate" type="xs:date">
							<xs:annotation>
								<xs:documentation>Дата формирования заявления</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="budget" type="tBudgetFunds" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Бюджет</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="budgetLevel" type="base:tBudgetLevel" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Уровень бюджета. Могут быть указаны значения:
1 - федеральный бюджет;
2 - бюджет субъекта Российской Федерации;
3 - местный бюджет</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="personalAccount" type="base:tAccountNumber" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Номер лицевого счёта</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="contractorInfo">
				<xs:annotation>
					<xs:documentation>Информация о поставщике</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice>
						<xs:element name="legalEntityInfo" type="tLegalEntity">
							<xs:annotation>
								<xs:documentation>Юридическое лицо</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="individualPersonInfo" type="tIndividualPerson">
							<xs:annotation>
								<xs:documentation>Индивидуальный предприниматеть</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:complexType>
			</xs:element>
			<xs:element name="foundationInfo">
				<xs:annotation>
					<xs:documentation>Основание</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="organizationInfo" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Организация, с которой клиент заключил  государственный (муниципальный) контракт (контракт, договор, соглашение)</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:choice minOccurs="0">
										<xs:element name="consRegistryNum" type="base:tSvRCode">
											<xs:annotation>
												<xs:documentation> Код по Сводному Реестру</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="NUBP" type="base:tNUBP">
											<xs:annotation>
												<xs:documentation>Код НУБП</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:choice>
									<xs:element name="INN" type="base:tINNOrganization" minOccurs="0">
										<xs:annotation>
											<xs:documentation>ИНН заказчика</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="fullName" type="base:tText2000" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Полное наименование заказчика</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="GRBSCode" type="tGRBS">
							<xs:annotation>
								<xs:documentation>ГРБС</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="contractInfo" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Информация о контракте</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="number" type="base:tContractTextCode" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Номер государственного контратка</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="IGK" type="base:tIGK" minOccurs="0">
										<xs:annotation>
											<xs:documentation>ИГК</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="conclusionDate" type="xs:date" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Дата заключения государственного контракта</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="regNum" type="base:tContractRegNum" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Номер реестровой записи контракта</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="link" type="base:tText2000" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Ссылка карточку контракта</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="signatoriesInfo">
				<xs:annotation>
					<xs:documentation>Информация о подписантах и подписании</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="headSignatureInfo" type="tSignatureInfo">
							<xs:annotation>
								<xs:documentation>Подпись руководителя</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="accountantSignatureInfo" type="tSignatureInfo" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Подпись бухгалтера</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="printFormInfo" type="tSignedData">
				<xs:annotation>
					<xs:documentation>Печатная форма документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="printFormFieldsInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дополнительная информация для печатной формы</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence minOccurs="0">
						<xs:element name="TOFKInfo" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Информация о ТОФК</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence minOccurs="0">
									<xs:element name="code" type="base:tTOFKCode" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Код ТОФК</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="name" type="base:tText2000" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Наименование ТОФК</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="foundation" type="base:tText2000" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Основание для открытия лицевого счета</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="schemeVersion" type="base:tSchemeVersion" use="required">
			<xs:annotation>
				<xs:documentation>Версия схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="tRequestResultPA">
		<xs:annotation>
			<xs:documentation>Тип: Результат рассмотрения заявления на резервирование/открытие лицевого счёта</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="refId" type="base:tGUID">
				<xs:annotation>
					<xs:documentation>Уникальный идентификатор заявления на резервирование (открытие) лицевого счёта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TOFK" type="base:tTOFKCode">
				<xs:annotation>
					<xs:documentation>Код ТОФК</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TOFKName" type="base:tText2000" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Наименование ТОФК</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="violations">
					<xs:annotation>
						<xs:documentation>В процессе обработки выявлены ошибки</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="violation" type="tViolation" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Ошибка</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="successRegister">
					<xs:annotation>
						<xs:documentation>ЛС успешно зарезервирован (открыт)</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Id" type="base:tGUID">
								<xs:annotation>
									<xs:documentation>Уникальный идентификатор извещения</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="receiptNumber" type="base:tProtocolNumber">
								<xs:annotation>
									<xs:documentation>Номер квитанции</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="registrationDT" type="xs:dateTime">
								<xs:annotation>
									<xs:documentation>Дата резервирования (открытия) ЛС</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="statementEDoc" type="tSignedData">
								<xs:annotation>
									<xs:documentation>Печатная форма квитанции в формате НСИ ЭБ (Формуляр)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="clientInfo" type="tClient">
								<xs:annotation>
									<xs:documentation>Сведения о клиенте</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="notMatchOrder">
					<xs:annotation>
						<xs:documentation>ЛС не открыт</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Id" type="base:tGUID">
								<xs:annotation>
									<xs:documentation>Уникальный идентификатор протокола</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="protocolNumber" type="base:tProtocolNumber">
								<xs:annotation>
									<xs:documentation>Номер протокола о несоответствии</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="createDT" type="xs:dateTime">
								<xs:annotation>
									<xs:documentation>Дата формирования сведений</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="rejectionReason" type="base:tText2000">
								<xs:annotation>
									<xs:documentation>Причина отказа</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="executorInfo" type="tExecutor">
								<xs:annotation>
									<xs:documentation>Сведения об ответственном исполнителе</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="notMatchEDoc" type="tSignedData">
								<xs:annotation>
									<xs:documentation>Печатная форма протокола о несоответствии в формате НСИ ЭБ (Формуляр)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="clientInfo" type="tClient">
								<xs:annotation>
									<xs:documentation>Сведения о клиенте</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="printFormFieldsInfo" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Дополнительная информация для печатной формы</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence minOccurs="0">
										<xs:element name="budgetName" type="base:tText2000" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Наименование бюджета</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="requestNum" type="base:tApplicationNum" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Номер заявления</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="generationDate" type="xs:date" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Дата формирования заявления</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="requestAcceptDate" type="xs:date" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Дата принятия заявления для обработки</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="schemeVersion" use="required">
			<xs:annotation>
				<xs:documentation>Версия схемы</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!--Вспомогательные типы-->
	<xs:complexType name="tAdressRF">
		<xs:annotation>
			<xs:documentation>Тип: Адрес РФ</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="kladrCode" type="base:tKLADRCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код КЛАДР</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="zip" type="base:tZip" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Почтовый индекс</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="regionCode" type="base:tRegionCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Код региона</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="regionInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Субъект РФ</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="fullName" type="base:tText1000" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Наименование</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="type" type="base:tKLADRObjCode" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Тип</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="areaInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Район</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="fullName" type="base:tText1000" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Наименование</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="type" type="base:tKLADRObjCode" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Тип</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="cityInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Город</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="fullName" type="base:tText1000" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Наименование</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="type" type="base:tKLADRObjCode" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Тип</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="settlementInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Населенный пункт</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="fullName" type="base:tText1000" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Наименование</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="type" type="base:tKLADRObjCode" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Тип</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="shortStreetInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Улица</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="fullName" type="base:tText1000" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Наименование</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="type" type="base:tKLADRObjCode" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Тип</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="building" type="base:tKLADRObjName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дом</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="housing" type="base:tKLADRObjName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Корпус (строение)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="flat" type="base:tKLADRObjName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Квартира</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tClient">
		<xs:annotation>
			<xs:documentation>Тип: Клиент в протоколе об отказе в открытии ЛС и квитанции</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="consRegistryNum" type="base:tSvRCode">
					<xs:annotation>
						<xs:documentation> Код по Сводному Реестру</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="NUBP" type="base:tNUBP">
					<xs:annotation>
						<xs:documentation>Код НУБП</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="fullName" type="base:tText2000" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Полное наименование</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="personalAccountNumber" type="base:tAccountNumber">
				<xs:annotation>
					<xs:documentation>Номер лицевого счёта</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="tConfirm">
		<xs:annotation>
